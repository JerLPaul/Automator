{
    "manifest_version": 3,
    "name": "Automator",
    "description": "Automates repetitive tasks",
    "version": "1.0",
    "content_scripts": 
        [{
            "matches": ["https://*/*"],
            "js": ["Content.js"]
        }],
    "action": 
    {
       "default_popup": "popup.html" ,
       "default_title": "Automator"
    } 
}